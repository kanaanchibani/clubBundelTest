{% extends "::base.html.twig" %}

{% block title %}<h1>Modif</h1>{% endblock %}

{% block body %}
    <center><h1>Welcome to the Club:update page</h1>
        <fieldset style="width: 50%">
            <form method="POST">
                NomC: <input type="text" name="NomC" value="{{ club.NomC }}"><br>
                MailC: <input type="text" name="MailC" value="{{ club.MailC }}"></br>
                IdUser: <input type="text" name="IdUser" value="{{ club.IdUser }}"></br>
                DescC: <input type="text" name="DescC" value="{{ club.DescC }}"></br>
                <input type="submit" value="Enregistrer">
            </form>
        </fieldset>
    </center>
{% endblock %}
